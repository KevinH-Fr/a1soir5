    <script>
      function formatDate(date) {
        var year = date.getFullYear();
        var month = ('0' + (date.getMonth() + 1)).slice(-2);
        var day = ('0' + date.getDate()).slice(-2);
        var hours = ('0' + date.getHours()).slice(-2);
        var minutes = ('0' + date.getMinutes()).slice(-2);
        var seconds = ('0' + date.getSeconds()).slice(-2);
        return year + '-' + month + '-' + day + 'T' + hours + ':' + minutes + ':' + seconds;
      }

      $(document).ready(function() {
        $('#start_time').change(function() {
          var startDate = new Date($(this).val()); // get value from start_time field
          startDate.setHours(startDate.getHours() + 1); // add one hour to start time
          $('#end_time').val(formatDate(startDate)); // if empty pass the value of start field
        });
      });
    </script>